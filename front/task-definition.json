{
  "family": "chucknorris-task-01",
  "containerDefinitions": [
    {
      "name": "chucknorris-frontend-container",
      "image": "chucknorris-frontend-ecr:latest",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 8081,
          "hostPort": 8081
        }
      ]
    }
  ],
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::328270397459:role/chucknorris-execution-task-role",
  "requiresCompatibilities": [
    "FARGATE"
  ]
}